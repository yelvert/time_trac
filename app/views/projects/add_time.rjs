text = ""
ProjectTime.find_all_by_project_id(@project.id, :order => "start_time DESC").each do |pt|
	text << "
	<tr id=\"project_time_#{pt.id}\">
		<td>#{pt.user.name}</td>
		<td>#{pt.start_time}</td>
		<td>#{pt.end_time.nil? ? "Running..." : pt.end_time}</td>
		<td>'+(new Time).fromElapsed(#{pt.elapsed_time}).prettyString+'</td>
	</tr>
	"
end
page << "$('project_times).update('#{text}');"
page << "$('manual_time_add').hide();"
page << "$('new_project_time').clear();"